<% provide(:title, '出勤社員一覧') %>
<div class="container">
  <h1>出勤中社員一覧</h1>
  <div class="row">
    <div class="col-md-10 col-md-offset-1">
      <table class="table table-hover" id="table-members-during-work">
        
        <thead>
          <tr>
            <td>社員番号</td>
            <td>名前</td>
          </tr>
        </thead>
        
        <tbody>
          <% @users_during_work.each do |user| %>
            <tr>
              <td><p><%= "#{user.employee_number}" %></p></td>
              <td><p><%= "#{user.name}" %></p></td>
            </tr>
          <% end %>
        </tbody>
        
      </table>
    </div>
  </div>
</div>